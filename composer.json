{
    "name": "itgalaxy/imagemin-php",
    "description": "A PHP port of Imagemin.",
    "keywords": [
        "minify",
        "compress",
        "image",
        "images",
        "jpeg",
        "jpg",
        "png",
        "gif",
        "svg",
        "webp"
    ],
    "homepage": "http://github.com/itgalaxy/imagemin-php",
    "license": "MIT",
    "authors": [
        {
            "name": "itgalaxy",
            "homepage": "https://itgalaxy.company"
        }
    ],
    "autoload": {
        "psr-4": {
            "Itgalaxy\\Imagemin\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "!bin",
            "tests",
            "*phpunit.xml*",
            ".travis.yml",
            "appveyor.yml",
            "composer.phar"
        ]
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/process": "^2.0 | ^3.0",
        "symfony/options-resolver": "^2.1 | ^3.0",
        "symfony/filesystem": "^2.0 | ^3.0",
        "itgalaxy/bin-wrapper": "^1.0",
        "psr/log": "^1.0.0",
        "itgalaxy/os-filter": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6.0",
        "phpunit/php-code-coverage": "^4.0.0"
    },
    "scripts": {
        "phpunit": "phpunit",

        "test": "@phpunit"
    }
}
